<%
   @per_page = session[:per_page] ? session[:per_page] : 24
   @lastpage = @amount/@per_page + 1

 %>

<div class="paging">
<% (1..@lastpage).each do |page| %>
    <a href="#"><span class="page"><%= page %></span></a>
<% end %>
</div>

<table id='report' class='<%= @report_engine.name.underscore %>_report'>

  <tr id='report-header'>
    <% @objects.first.attrs.keys.each do |field| %>
        <th><%= field %></th>
    <% end %>
  </tr>

  <% @objects.each do |object| %>
      <tr>
        <% object.attrs.values.each do |value| %>
            <td><%= value %></td>
        <% end %>
      </tr>
  <% end %>


  <tr id='report-footer' data-all='<%= @amount %>'>
    All: <%= @amount %>,visible: <%= @objects.length %>
  </tr>



</table>